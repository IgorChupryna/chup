<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Quiz</servlet-name>
		<servlet-class>com.chup.QuizServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Quiz</servlet-name>
		<url-pattern>/quiz</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Stat</servlet-name>
		<servlet-class>com.chup.StatServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Stat</servlet-name>
		<url-pattern>/stat</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.chup.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
